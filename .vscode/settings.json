{
  // 开启预览模式
  "workbench.editor.enablePreview": false,
  // 设置 eslint 检查的文件类型
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  // 设置保存后的行为
  "editor.codeActionsOnSave": {
    // 设置 eslint 自动修复可修复的规则
    "source.fixAll.eslint": true
  }
}
